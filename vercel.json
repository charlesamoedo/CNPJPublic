{
  "version": 2,
  "builds": [
    {
      "src": "server/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/proxy.js"
    },
    {
      "src": "/.*",
      "dest": "public/index.html"
    }
  ],
  "env": {
    "PORT": "3000",
    "NODE_ENV": "production"
  }
}
