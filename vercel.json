{
  "version": 2,
  "builds": [
    {
      "src": "server/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/proxy.js"
    },
    {
      "src": "(.*)",
      "dest": "server/proxy.js"
    }
  ],
  "env": {
    "PORT": "3000",
    "NODE_ENV": "production"
  }
}
